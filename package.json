{
  "name": "koop-provider-nemo",
  "version": "1.2.2",
  "description": "A NEMO provider for Koop",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "koop serve",
    "test": "standard && tape test/*.js | tap-spec",
    "format": "prettier --write '{src,test}/**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "axios": "0.19.2",
    "config": "3.3.1",
    "koop": "4.0.1",
    "lodash": "4.17.20"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@koopjs/cli": "0.6.3",
    "husky": "4.2.5",
    "nock": "13.0.4",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "standard": "14.3.4",
    "tap-spec": "5.0.0",
    "tape": "5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cooperka/koop-provider-nemo.git"
  },
  "keywords": [
    "koop",
    "provider",
    "nemo"
  ]
}
